{
	"extends": "./jsconfig.paths.json",
    "compilerOptions": {
        "paths": {
			"@assets/*": ["src/js/assets/*"],
			"@components/*": ["src/components/*"],
			"@common/*": ["src/common/*"],
			"@gis/*": ["src/cesium/*"],
			"@redux/*": ["src/redux/*"],
			"@images/*": ["src/resources/images/*"]
		}
    },
    "include": [
        "src",
        "craco.config.js"
	]
}
